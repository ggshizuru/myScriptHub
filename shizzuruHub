    function dowait()
			wait(0.172)
		end
		local tbl_main = 
		{
			  "EZZZZZZZZZZ" , 
			  "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(tbl_main))
		dowait()
		local tbl_main = 
		{
			  "ShizzuruHub" , 
			  "All"
		}
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(tbl_main))
		dowait()
		local tbl_main = 
		{
			  "wins." , 
			  "All"
		}
